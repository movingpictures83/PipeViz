# This script will fetch all input files from a configuration file. 
# Then it will find all files that contain a line 'inputfile filename' and will check that no files are in startFiles.txt

if [[ $# != 1 ]]
then
	echo "Invalid number of arguments. Expected 1 got ${#}."
	echo "Usage: 'parameter_files [file]'"
	exit 1
fi

for file in `plugin_info $1 inputs`
do
	for file ! in 'plugin_info $1 outputs'
	do
		find startFiles.txt -not -name $file | (egrep -ls '^inputfile{1}	.*' $file)
	done
done
